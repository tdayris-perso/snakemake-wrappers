        time_min = (
            lambda wildcards, attempt: min(attempt * 20, 200)
        )
    params:
        extra = "plot_type = 'violin'"
    log:
        "logs/test_pcaexplorer_distro_expr.log"
    wrapper:
        "master/bio/pcaExplorer/distro_expr"

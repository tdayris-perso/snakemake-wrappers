rule test_pcaexplorer_distro_expr:
    input:
        dst = "vst.RDS"
    output:
        png = "plot.png"
    message:
        "Testing pcaExplorer distro_expr"
    threads:
        1
    resources:
        mem_mb = (
            lambda wildcards, attempt: min(attempt * 1024, 10240)
